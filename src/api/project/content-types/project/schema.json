    },
    "price": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::price.price",
      "mappedBy": "project"
    },
    "review": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::review.review",
      "mappedBy": "project"
    },
    "benefit": {
      "type": "relation",
      "relation": "oneToOne",
