    },
    "overview": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::overview.overview",
      "mappedBy": "project"
    },
    "page_modules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page-module.page-module"
    }
